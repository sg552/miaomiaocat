<h1>租房信息列表</h1>

<table class="table table-striped">
  <thead>
  <tr>
    <th>来源网站</th>
    <th>信息提要</th>
    <th>抓取日期</th>
  </tr>
  </thead>

  <tbody>
<% @items.each do |item| %>
  <tr>
    <td><%= link_to item.source_website.try(:name), item.source_website.try(:url), :target => "_blank" %></td>
    <td><%= link_to item.content, item.original_url, :target => "_blank" %> </td>
    <td><%= item.created_at %> </td>
  </tr>
<% end %>
  </tbody>
</table>

